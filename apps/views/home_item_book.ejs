<% items.forEach(function(item){%>
    <div class="col-sm-4">
        <div class="product-image-wrapper">
            <div class="single-products">
                <div class="productinfo text-center" id="<%= item.id_sach%>">
                    <a href="/book/<%= item.id_sach%>">
                        <img src="static/imgs/category/books/<%=item.image_sach_url%>" alt="" height="300" />
                    </a>
                    <h2><%= item.gia%> Ä‘</h2>
                    <p><%= item.ten_sach%></p>
                    <p><%= item.so_luong_ton%></p>                  
                </div>
                
            </div>

            <div class="ml-3">
                <button class="btn btn-default add-to-cart"><i class="fa fa-shopping-cart"></i>Add to cart</button>
                
                <a href="#" class="btn btn-default add-to-cart"><i class="fa fa-calendar-check"></i>Watch later</a>
            </div>
        </div>
    </div>
<%});%>

